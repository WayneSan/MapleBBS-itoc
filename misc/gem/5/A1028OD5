#include "bbs.h"

typedef struct {....} OLD;
typedef struct {....} NEW;

int main()
{
  int num;
  OLD old;
  NEW new;

  num = 0;
  while (!rec_get("old-file", &old, sizeof(OLD), num))
  {
    new.xxx = old.xxx;              /* ¦pªG xxx Äæ¦ì¬O int */
    strcpy(new.yyy, old.yyy);       /* ¦pªG yyy Äæ¦ì¬O char[] */
    rec_add("new-file", &new, sizeof(NEW));
    num++;
  }
}

