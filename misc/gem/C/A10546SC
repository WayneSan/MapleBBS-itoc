static int
brdtitle_cmp(i, j)              /* itoc.010413: ¨Ì¬ÝªO¤¤¤å±Ô­z±Æ§Ç */
  short *i, *j;
{
  int iSeq, jSeq;

  iSeq = sequence_match(bhead[*i].class);
  jSeq = sequence_match(bhead[*j].class);

  if (!iSeq && !jSeq)
  {
    /* ³£¤£¬O¡u«ü©wÀu¥ý¶¶§Ç¤ÀÃþ¡v´N¤ÀÃþ/ªO¦W¥æ¤e¤ñ¹ï */
    iSeq = strcmp(bhead[*i].class, bhead[*j].class);
    return iSeq ? iSeq : str_cmp(bhead[*i].brdname, bhead[*j].brdname);
  }
  else if (!iSeq)
  {
    /* «D¡u«ü©wÀu¥ý¶¶§Ç¤ÀÃþ¡v±Æ§Ç¤@©w¦b«á */
    return 1;
  }
  else if (!jSeq)
  {
    /* ¬O¡u«ü©wÀu¥ý¶¶§Ç¤ÀÃþ¡v±Æ§Ç¤@©w¦b«e */
    return -1;
  }
  else
  {
    /* ³£¬O¡u«ü©wÀu¥ý¶¶§Ç¤ÀÃþ¡v¡A¥ý¤ñ¹ï¶¶§Ç¡A¦A¤ñ¹ïªO¦W */
    return iSeq != jSeq ? iSeq - jSeq :
      str_cmp(bhead[*i].brdname, bhead[*j].brdname);
  }
}

