: gem.c  gem_state()©³¤U·s¼W

static int
hdr_cmp(a, b)
  HDR *a;
  HDR *b;
{
  /* ¥ý¤ñ¹ï¤ÀÃþ¡A¦A¤ñ¹ïªO¦W */
  int k = strncmp(a->title + BNLEN + 1, b->title + BNLEN + 1, BCLEN);
  return k ? k : str_cmp(a->xname, b->xname);
}


static int
gem_sort(xo)
  XO *xo;
{
  if (!HAS_PERM(PERM_ALLBOARD))
    return XO_NONE;

  if (xo->key == GEM_VISIT)         /* ¦b gopher ¤¤ */
    return XO_NONE;

  rec_sync(xo->dir, sizeof(HDR), hdr_cmp, NULL);

  return gem_init(xo);
}


: gem.c  gem_cb[]


'S', gem_state,
+Ctrl('S'),gem_sort, /*«öÁä¦Û­q*/


