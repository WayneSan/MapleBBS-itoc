: nocem.c:NCMupdate()

static void
NCMupdate(issuer, type)
  char *issuer, *type;
{
  ncmperm_t ncm;

  memset(&ncm, 0, sizeof(ncmperm_t));
  str_ncpy(ncm.issuer, issuer, sizeof(ncm.issuer));
  str_ncpy(ncm.type, type, sizeof(ncm.type));
- ncm.perm = 0;
+ ncm.perm = 1;
  rec_add("innd/ncmperm.bbs", &ncm, sizeof(ncmperm_t));
  read_ncmperm();
}


