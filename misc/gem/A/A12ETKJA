: board.c:class_allvisit() ©ñ¦b class_unvisit() «á­±

static int
class_allvisit(xo)
  XO *xo;
{
  BRD *brd;
  short *chp;
  int ans, pos, chn, max;

  ans = vans("³]©w©Ò¦³¬ÝªO (V)¤wÅª (U)¥¼Åª (Q)¨ú®ø¡H [Q] ");
  if (ans != 'v' && ans != 'u')
    return XO_FOOT;

  max = xo->max;
  pos = 0;
  do
  {
    chp = (short *) xo->xyz + pos;
    chn = *chp;
    if (chn >= 0)
    {
      brd = bshm->bcache + chn;
      brh_get(brd->bstamp, chn);
      if (ans == 'v')
      {
        brh_visit(0);
#ifndef ENHANCED_VISIT
        time(&brd_visit[chn]);
#endif
      }
      else
      {
        brh_visit(1);
#ifndef ENHANCED_VISIT
        brd_visit[chn] = 0; /* itoc.010402: ³ÌªñÂsÄý®É¶¡Âk¹s¡A¨Ï¬ÝªO¦Cªí
                                            ¤¤Åã¥Ü¥¼Åª */
#endif
      }
    }
  } while (++pos < max);

  return class_body(xo);
}

